    <Pregunta texto = "Diagrama de sintaxis que representa opción entre una cadena A y otra cadena B:">
        <respuesta tipo = "Correcta">/Imagenes/e_opcion1.png</respuesta>
        <respuesta>/Imagenes/e_yuxtaposicion.png</respuesta>
        <respuesta>/Imagenes/e_opcion2.png</respuesta>     
    </Pregunta>


    <Pregunta texto = "Diagrama de sintaxis que representa opción entre una cadena B y una cadena vacía:">
        <respuesta tipo = "Correcta">/Imagenes/e_opcion2.png</respuesta>
        <respuesta>/Imagenes/e_opcion1.png</respuesta>
        <respuesta>/Imagenes/e_repetir1.png</respuesta>
    </Pregunta>


    <Pregunta texto = "Diagrama de sintaxis que representa repetición de una o más veces B:">
        <respuesta tipo = "Correcta">/Imagenes/e_repetir1.png</respuesta>
        <respuesta>/Imagenes/e_opcion2.png</respuesta>
        <respuesta>/Imagenes/e_repetir2.png</respuesta>
    </Pregunta>


    <Pregunta texto = "Diagrama de sintaxis que representa repetición de cero o más veces B:">
        <respuesta tipo = "Correcta">/Imagenes/e_repetir2.png</respuesta>
        <respuesta>/Imagenes/e_repetir1.png</respuesta>
        <respuesta>/Imagenes/e_opcion1.png</respuesta>
    </Pregunta>

    <Pregunta texto = "Cadena aceptada por el siguiente diagrama de sintaxis:">
        <imagen>/Imagenes/e_yuxtaposicion.png</imagen>
        <respuesta tipo = "Correcta">AB</respuesta>
        <respuesta>AA</respuesta>
        <respuesta>BA</respuesta>
    </Pregunta>
    
      <Pregunta texto = "Cadena(s) aceptada(s) por el siguiente diagrama de sintaxis:">
        <imagen>/Imagenes/e_opcion1.png</imagen>
        <respuesta tipo = "Correcta">A y B</respuesta>
        <respuesta>A y Cadena Vacía</respuesta>
