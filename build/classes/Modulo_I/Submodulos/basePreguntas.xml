    </Pregunta>

  <Pregunta texto = "Cadena(s) aceptada(s) por el siguiente diagrama de sintaxis:">
        <imagen>/Imagenes/e_opcion2.png</imagen>
        <respuesta tipo = "Correcta">B y Cadena Vacía</respuesta>
        <respuesta>A y B</respuesta>
        <respuesta>Sólo B</respuesta>
    </Pregunta>
    
      <Pregunta texto = "Cadena(s) aceptada(s) por el siguiente diagrama de sintaxis:">
